

.PHONY: all copy clean

#*****************************************************************************#
#                               Source FILE LIST                              #
#*****************************************************************************#
CSRC        = src inc
		


#*****************************************************************************#
#                        RULES TO GENERATE TARGETS                            #
#*****************************************************************************#

# Define the Rules to build the core targets
all: copy
copy:
		for cpf in $(CSRC); do \
			cp -rf $$cpf ../..; \
		done	 
	


